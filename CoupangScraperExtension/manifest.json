{
  "manifest_version": 3,
  "name": "Coupang Auto Scraper",
  "version": "1.0",
  "description": "쿠팡 상품 페이지 접속 시 자동으로 상품 정보를 JSON으로 추출합니다.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.coupang.com/vp/products/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}